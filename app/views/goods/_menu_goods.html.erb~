<ul>
	<% Category.all.each do |r| %>
		<%if r.super_category == ""%>
			<% temp = "" %>
			<% if r.sub_category != "" %>
				<% r.sub_category.to_s.split(',').each do |p| %>
					<% temp += Category.find(p.to_i).title.to_s + ","%>
				<% end %>
			<% end %>
			<a onclick=<%='MyOnClick("'+r.id.to_s+'d","'+temp.to_s+'")'%> style="font-size:12pt"><%= r.title%><div id='<%= r.id.to_s+"d"%>'></div>
       			
        	<%end%> 
	<%end%>
</ul>
